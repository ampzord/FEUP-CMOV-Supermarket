<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/regular_background"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="91dp"
        android:layout_height="29dp"
        android:fontFamily="@font/archivo_black"
        android:text="Profile"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.229"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="725dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="91dp"
        android:layout_height="29dp"
        android:fontFamily="@font/archivo_black"
        android:text="Logout"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.776"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="725dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="363dp"
        android:layout_height="191dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="188dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/cart_back_button"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="61dp"
        android:layout_height="51dp"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backarrow" />

    <ImageButton
        android:id="@+id/bt_scan_item"
        android:layout_width="120dp"
        android:layout_height="115dp"
        android:layout_marginTop="32dp"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qrbutton" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="91dp"
        android:layout_height="29dp"
        android:fontFamily="@font/archivo_black"
        android:text="Scanner"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_scan_item" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="91dp"
        android:layout_height="29dp"
        android:fontFamily="@font/archivo_black"
        android:text="Scanner"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_scan_item" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="105dp"
        android:layout_height="31dp"
        android:fontFamily="@font/archivo_black"
        android:text="Checkout"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.195"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="536dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="91dp"
        android:layout_height="29dp"
        android:fontFamily="@font/archivo_black"
        android:text="Coupons"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.779"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="536dp" />

    <ImageButton
        android:id="@+id/coupons_button"
        android:layout_width="140dp"
        android:layout_height="140dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/checkout_button"
        app:srcCompat="@drawable/pic_coupon"
        tools:layout_editor_absoluteY="403dp" />

    <ImageButton
        android:id="@+id/checkout_button"
        android:layout_width="140dp"
        android:layout_height="140dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pic_checkout"
        tools:layout_editor_absoluteY="403dp" />

    <ImageButton
        android:id="@+id/profile_button"
        android:layout_width="140dp"
        android:layout_height="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkout_button"
        app:srcCompat="@drawable/pic_user"
        tools:layout_editor_absoluteX="48dp" />

    <ImageButton
        android:id="@+id/logout_button"
        android:layout_width="140dp"
        android:layout_height="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/profile_button"
        app:layout_constraintTop_toBottomOf="@+id/coupons_button"
        app:srcCompat="@drawable/pic_logout" />

</androidx.constraintlayout.widget.ConstraintLayout>